{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --cache --fix",
    "test": "jest --passWithNoTests",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.2.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/memory-cache": "^0.2.5",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
